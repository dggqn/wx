/* pages/applyPages/collarInvoiceDetail/collarInvoiceDetail.wxss */
.collar-steps {
  height: 130rpx;
  background: #fff;
}
.collar-steps .step-box {
  text-align: center;
  position: relative;
}
.collar-steps .step-box .step{
  width: 40rpx;
  height: 40rpx;
  text-align: center;
  line-height: 40rpx;
  border-radius: 50%;
  background: #6C66FA;
  color: #fff;
  font-size: 24rpx;
  margin: 0 auto;
  position: relative;
  z-index: 1000;
}
.collar-steps .step-box::before{
  content: '';
  position: absolute;
  left: 0;
  top: 18rpx;
  width: 50%;
  height: 4rpx;
  background: #6C66FA;
}
.collar-steps .step-box:first-child::before{
  display: none;
}
.collar-steps .step-box::after{
  content: '';
  position: absolute;
  right: 0;
  top: 18rpx;
  height: 4rpx;
  width: 50%;
  background: #6C66FA;
}
.collar-steps .step-box:last-child::after {
  display: none;
}
.step-text{
  font-size: 24rpx;
  color: #6C66FA;
  margin-top: 5rpx;
}

/* 资产列表 */
.assets-container {
  padding: 0 30rpx;
  padding-bottom: 30rpx;
  box-sizing: border-box;
}
.assets-container .assets-total{
  padding-top: 28rpx;
  color: #9494C3;
  font-size: 24rpx;
}
.assets-container .list-item {
  margin-top: 24rpx;
}
.list-item .item-title{
  width: 100%;
  background: #FBFBFF;
  height: 72rpx;
  line-height: 72rpx;
  padding: 0 24rpx;
  box-sizing: border-box;
}
.list-item .item-title .title-name{
  color: #30304C;
}
.list-item .item-title .title-num {
  color: #9090A8;
  font-size: 24rpx;
}
.list-item .item-content {
  padding: 12rpx 24rpx 24rpx;
  background: #fff;
}
.list-item .item-content.assets-has-sign-item{
background: url('data:image/png;base64,') #fff no-repeat;
background-size: 134rpx 134rpx;
background-position: top right;
}
.list-item .item-content .content-img{
  width: 140rpx;
  height: 140rpx;
  margin-right: 24rpx;
}
.list-item .item-content .content-detail {
  justify-content: space-between;
}
.list-item .item-content .content-detail .detail-top {
  color: #6F6F8E;
  font-size: 24rpx;
}
.content-detail .detail-bottom .operate-btn{
  display: inline-block;
  width: 128rpx;
  height: 50rpx;
  background: #5EA1F2;
  line-height: 50rpx;
  text-align: center;
  border-radius: 4rpx;
  color: #fff;
  font-size: 24rpx;
}

/* 耗材明细 */
.consum-container {
  padding: 0 30rpx;
}
.consum-item {
  margin-top: 24rpx;
  background: #fff;
  border-radius: 16rpx;
  overflow: hidden;
}
.consum-item.consum-has-sign-item {
  background: url('data:image/png;base64,') #fff no-repeat;
  background-size: 150rpx 150rpx;
  background-position: left bottom;
}
.consum-item .item-title {
  height: 54rpx;
  line-height: 54rpx;
  color: #9494C3;
  font-size: 24rpx;
  padding: 0 30rpx;
  background: #FBFBFF;
}
.consum-item .item-content {
  padding: 0 30rpx;
  padding-bottom: 24rpx;
}
.consum-item .item-content .content-title{
  padding: 20rpx 0;
  color: #30304C;
  font-size: 24rpx;
  border-bottom: 1rpx dashed #6F6F8E;
  font-weight: 500;
}
.consum-item .item-content .content-list {
  color: #30304C;
  font-size: 24rpx;
}
.consum-item .item-content .content-list .list-item{
  margin-top: 24rpx;
}
.consum-item .item-content .content-spec,
.consum-item .item-content .content-num {
  width: 120rpx;
  text-align: center;
}
.consum-item .item-content .content-btn{
  text-align: right;
  margin-top: 24rpx;
}
.consum-item .item-content .content-btn .operate-btn {
  display: inline-block;
  width: 138rpx;
  height: 50rpx;
  text-align: center;
  line-height: 50rpx;
  border-radius: 4rpx;
  font-size: 24rpx;
  background: #5EA1F2;
  color: #fff;
}
/* 身份二维码 */
.consum-container .person-code{
  width: 140rpx;
  height: 140rpx;
  position: fixed;
  right: 0;
  bottom: 280rpx;
  background: #fff;
  color: #30304C;
  font-size: 20rpx;
  border-radius: 50%;
  justify-content: center;
  align-items: center;
}
.consum-container .person-code .small-code-box{
  width: 48rpx;
  height: 48rpx;
  margin-top: 8rpx;
}

/* 底部操作 */
.collar-bottom .collar-time-limit{ 
  height: 54rpx;
  line-height: 54rpx;
  text-align: center;
  background: #6F6F8E;
  color: #fff;
}
.collar-bottom .bottom-operates{
  height: 96rpx;
  line-height: 96rpx;
  color: #fff;
  text-align: center;
}
.collar-bottom .bottom-operates .operate-l{
  background: #6C66FA
}
.collar-bottom .bottom-operates .operate-r{
  background: #485995
}