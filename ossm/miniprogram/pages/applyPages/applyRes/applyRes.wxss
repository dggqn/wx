.car-list-container {
  position: relative;
  max-height: 1100rpx;
}
.car-list-container .tip {
  padding: 32rpx;
  padding-bottom: 0;
}
.car-list-container .tip .clear-car {
  color: #DB00BF;
}
.car-list-container .car-list {
  padding: 20rpx 32rpx;
  padding-bottom: 170rpx;
}
.car-list-container .list-item {
  border: 1rpx solid #6F6F8E;
  border-radius: 4rpx;
  padding: 24rpx;
  box-sizing: border-box;
  margin-top: 24rpx;
}
.car-list-container .list-item .item-img {
  width: 160rpx;
  height: 160rpx;
  margin-right: 16rpx;
}
.car-list-container .list-item .item-r{
  width: 458rpx;
}
.car-list-container .list-item .item-title {
  color: #30304C;
  font-size: 32rpx;
  height: initial;
}
.list-item .item-title{
  line-height: inherit;
}
.car-list-container .list-item .item-spec {
  margin-top: 10rpx;
  padding: 0 24rpx;
}
.car-list-container .list-item .item-spec .spec {
  display: inline-block;
  margin-right: 20rpx;
  width: 438rpx;
  font-size: 24rpx;
}
.car-list-container .list-item .item-num{
  margin-top: 6rpx;
}
.car-list-container .list-item .item-remark {
  font-size: 24rpx;
  color: #9494C3;
  line-height: 33rpx;
}
.car-list-container .list-item .item-num .item-num-cut,
.car-list-container .list-item .item-num .item-num-add{
  width: 50rpx;
  height: 50rpx;
}
.car-list-container .list-item .item-num .item-num-ipt {
  text-align: center;
}
