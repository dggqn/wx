/* 资产变更与报废进度条 */
.czh-steps-vertical .czh-steps-vertical-item .img{
  width: var(--width);
  height: var(--width);
  border-radius: 50%;
  border: 1rpx solid #eaeaea;
}
.czh-steps-vertical .czh-steps-vertical-item .introduce{
  margin-left: 4rpx;
  white-space: nowrap;
}
.czh-steps-vertical .czh-steps-vertical-item .introduce .introduce-item1{
  font-size: 24rpx;
  color: #1C3A62;
}
.czh-steps-vertical .czh-steps-vertical-item .introduce .introduce-item2{
  font-size: 20rpx;
  color: rgba(28, 58, 98, .5);
}
.czh-steps-vertical .line{
  margin: 4rpx calc( var(--width) / 2 );
  width: 2rpx;
  background-color: rgba(28, 58, 98, .3);
}
